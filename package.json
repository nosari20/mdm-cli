{
  "name": "cli",
  "version": "0.0.0",
  "main": "./electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href && electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href && electron .",
    "electron:win-build": "ng build --base-href --prod && electron-packager . Cli --icon=src/favicon.ico --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=electron/build/prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "electron-store": "^2.0.0",
    "get-ssl-certificate": "^2.3.1",
    "ngx-electron": "^2.1.1",
    "rxjs": "~6.3.3",
    "sshpk": "^1.16.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.2",
    "electron-packager": "^13.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "build": {
    "appId": "cli",
    "category": "CLI",
    "win": {
      "target": "squirrel",
      "icon": "src/icon.png"
    }
  }
}
